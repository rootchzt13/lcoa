function getter(){
var i = 0;
// include std.sh DONT NOT EDIT FARIZ COPYRIGTH
document.getElementsByClassName('next-radio ').item(8).click()

function test(){


	setTimeout(()=>{
		// clicking element step 1
		document.getElementsByClassName('next-select medium multiple').item(0).click()



		// clicking size s
		document.getElementsByClassName('next-menu-item').item(19).click()

		// size m
		document.getElementsByClassName('next-menu-item').item(17).click()

		// size L
		document.getElementsByClassName('next-menu-item').item(15).click()

		// size xl

		document.getElementsByClassName('next-menu-item').item(21).click()

		// size xxl
		document.getElementsByClassName('next-menu-item').item(24).click()

		i++;

		if( i < 1 ){
 
			test()
		}
	},100)
}


// element group-top

var btn = document.createElement('button')

document.getElementsByClassName('group-top').item(0).appendChild(btn)



// btn.onclick = function() {
// 	test()
// }

btn.addEventListener('click', test())
}



